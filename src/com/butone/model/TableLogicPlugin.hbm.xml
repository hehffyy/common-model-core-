<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.butone.model.TableLogicPlugin"
        table="M_TableLogicPlugin"
    >

        <id
            name="guid"
            column="fguid"
            type="java.lang.String"
            length="32"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-TableLogicPlugin.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <version
            name="verNum"
            column="FVerNum"
            type="java.lang.Integer"
        />

        <property
            name="event"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FEvent"
            length="20"
            not-null="true"
        />

        <property
            name="tableGuid"
            type="java.lang.String"
            update="true"
            insert="true"
        >
            <column
                name="FTableGuid" 
                length="32"
                unique-key="M_TableLogicPlugin_Name_Table"
                not-null="true"
            />
        </property>

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
        >
            <column
                name="FName" 
                length="100"
                unique-key="M_TableLogicPlugin_Name_Table"
                not-null="true"
            />
        </property>

        <property
            name="className"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FLogicClass"
            length="100"
        />

        <property
            name="parameter"
            type="org.springframework.orm.hibernate3.support.BlobStringType"
            update="true"
            insert="true"
            column="FParameter"
        />

        <property
            name="dispName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FDispName"
            length="100"
            not-null="true"
        />

        <property
            name="relBizDatas"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FRelBizDatas"
            length="1000"
        />

        <property
            name="desc"
            type="java.lang.String"
            update="true"
            insert="true"
            column="FDesc"
            length="200"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-TableLogicPlugin.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
